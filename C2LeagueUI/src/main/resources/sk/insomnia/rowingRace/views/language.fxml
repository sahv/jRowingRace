<?import java.lang.String?>
<?import java.net.URL?>
<AnchorPane id="dialogWindow" prefHeight="121.0" prefWidth="333.0" xmlns:fx="http://javafx.com/fxml" fx:controller="sk.insomnia.rowingRace.ui.controller.LanguageController">
  <!-- TODO Add Nodes -->
  <children>
    <GridPane prefHeight="121.0" prefWidth="333.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <Label id="labelBiggerWhite" prefWidth="166.5" text="%label.language" GridPane.columnIndex="0" GridPane.rowIndex="0">
          <stylesheets>
            <URL value="@../css/rowingRace.css" />
          </stylesheets>
          <GridPane.margin>
            <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" fx:id="x1" />
          </GridPane.margin>
        </Label>
        <ComboBox fx:id="cbLanguage" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.margin="$x1" GridPane.rowIndex="0" GridPane.valignment="CENTER">
          <items>
            <FXCollections fx:factory="observableArrayList">
              <String fx:value="Item 1" />
              <String fx:value="Item 2" />
              <String fx:value="Item 3" />
            </FXCollections>
          </items>
        </ComboBox>
        <Button contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleConfirm" text="%label.control.confirm" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.margin="$x1" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
